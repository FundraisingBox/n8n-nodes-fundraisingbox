{
  "name": "@fundraisingbox/n8n-nodes-fundraisingbox",
  "version": "0.1.0",
  "description": "n8n community node for FundraisingBox",
  "license": "MIT",
  "homepage": "",
  "keywords": ["n8n-community-node-package"],
  "author": { "name": "Patricia Montag", "email": "p.montag@fundraisingbox.com" },
  "repository": {
    "type": "git",
    "url": "https://github.com/fundraisingbox/n8n-nodes-fundraisingbox.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": ["dist"],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/FundraisingBoxApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/FundraisingBox/Fundraisingbox.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": { "n8n-workflow": "*" }
}
